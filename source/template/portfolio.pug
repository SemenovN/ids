									table
										tr
											td(colspan="2") Количество установок
										tr
											td
												div.project__install
													+icon('apple-logo')
													span 833.060
											td
												+icon_link('google-play', '#')
										tr
											td
												div.project__install
													+icon('android-logo')
													span 47.990
											td
												+icon_link('app-store', '#')
